<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
    <xacro:macro name="insert_robot" params="prefix parent *origin">
        <link name="${prefix}base_link">
            <visual>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <geometry>
                    <mesh filename="package://cs66_description/meshes/visual/base.dae" scale="1 1 1"/>
                </geometry>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <geometry>
                    <mesh filename="package://cs66_description/meshes/visual/base.dae" scale="1 1 1"/>
                </geometry>
            </collision>
            <inertial>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <mass value="1.0"/>
                <inertia ixx="1.0" ixy="0" ixz="0" iyy="1.0" iyz="0" izz="1.0"/>
            </inertial>
        </link>

        <joint name="base_link_fixed_joint" type="fixed">
            <parent link="${parent}"/>
            <child link="${prefix}base_link"/>
            <xacro:insert_block name="origin" />
        </joint>

        <link name="${prefix}link1">
            <visual>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <geometry>
                    <mesh filename="package://cs66_description/meshes/visual/link1.dae" scale="1 1 1"/>
                </geometry>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <geometry>
                    <mesh filename="package://cs66_description/meshes/visual/link1.dae" scale="1 1 1"/>
                </geometry>
            </collision>
            <inertial>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <mass value="1.0"/>
                <inertia ixx="1.0" ixy="0" ixz="0" iyy="1.0" iyz="0" izz="1.0"/>
            </inertial>
        </link>

        <joint name="${prefix}joint1" type="revolute">
            <parent link="${prefix}base_link"/>
            <child link="${prefix}link1"/>
            <origin xyz="0 0 0.0919" rpy="0 0 0"/>
            <axis xyz="0 0 1"/>
            <limit effort="1000" velocity="1.0" lower="${-pi}" upper="${pi}"/>
        </joint>

        <link name="${prefix}link2">
            <visual>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <geometry>
                    <mesh filename="package://cs66_description/meshes/visual/link2.dae" scale="1 1 1"/>
                </geometry>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <geometry>
                    <mesh filename="package://cs66_description/meshes/visual/link2.dae" scale="1 1 1"/>
                </geometry>
            </collision>
            <inertial>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <mass value="1.0"/>
                <inertia ixx="1.0" ixy="0" ixz="0" iyy="1.0" iyz="0" izz="1.0"/>
            </inertial>
        </link>

        <joint name="${prefix}joint2" type="revolute">
            <parent link="${prefix}link1"/>
            <child link="${prefix}link2"/>
            <origin xyz="0 -0.0704 0.0706" rpy="0 0 ${pi}"/>
            <axis xyz="0 1 0"/>
            <limit effort="1000" velocity="1.0" lower="${-pi}" upper="${pi}"/>
        </joint>

        <link name="${prefix}link3">
            <visual>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <geometry>
                    <mesh filename="package://cs66_description/meshes/visual/link3.dae" scale="1 1 1"/>
                </geometry>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <geometry>
                    <mesh filename="package://cs66_description/meshes/visual/link3.dae" scale="1 1 1"/>
                </geometry>
            </collision>
            <inertial>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <mass value="1.0"/>
                <inertia ixx="1.0" ixy="0" ixz="0" iyy="1.0" iyz="0" izz="1.0"/>
            </inertial>
        </link>

        <joint name="${prefix}joint3" type="revolute">
            <parent link="${prefix}link2"/>
            <child link="${prefix}link3"/>
            <origin xyz="0.427 0.0034 0" rpy="0 0 0"/>
            <axis xyz="0 1 0"/>
            <limit effort="1000" velocity="1.0" lower="${-pi}" upper="${pi}"/>
        </joint>

        <link name="${prefix}link4">
            <visual>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <geometry>
                    <mesh filename="package://cs66_description/meshes/visual/link4.dae" scale="1 1 1"/>
                </geometry>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <geometry>
                    <mesh filename="package://cs66_description/meshes/visual/link4.dae" scale="1 1 1"/>
                </geometry>
            </collision>
            <inertial>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <mass value="1.0"/>
                <inertia ixx="1.0" ixy="0" ixz="0" iyy="1.0" iyz="0" izz="1.0"/>
            </inertial>
        </link>

        <joint name="${prefix}joint4" type="revolute">
            <parent link="${prefix}link3"/>
            <child link="${prefix}link4"/>
            <origin xyz="0.3905 -0.0118 0" rpy="0 0 0"/>
            <axis xyz="0 1 0"/>
            <limit effort="1000" velocity="1.0" lower="${-pi}" upper="${pi}"/>
        </joint>

        <link name="${prefix}link5">
            <visual>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <geometry>
                    <mesh filename="package://cs66_description/meshes/visual/link5.dae" scale="1 1 1"/>
                </geometry>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <geometry>
                    <mesh filename="package://cs66_description/meshes/visual/link5.dae" scale="1 1 1"/>
                </geometry>
            </collision>
            <inertial>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <mass value="1.0"/>
                <inertia ixx="1.0" ixy="0" ixz="0" iyy="1.0" iyz="0" izz="1.0"/>
            </inertial>
        </link>

        <joint name="${prefix}joint5" type="revolute">
            <parent link="${prefix}link4"/>
            <child link="${prefix}link5"/>
            <origin xyz="0 0.0855 -0.049" rpy="0 ${pi} 0"/>
            <axis xyz="0 0 1"/>
            <limit effort="1000" velocity="1.0" lower="${-pi}" upper="${pi}"/>
        </joint>

        <link name="${prefix}link6">
            <visual>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <geometry>
                    <mesh filename="package://cs66_description/meshes/visual/link6.dae" scale="1 1 1"/>
                </geometry>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <geometry>
                    <mesh filename="package://cs66_description/meshes/visual/link6.dae" scale="1 1 1"/>
                </geometry>
            </collision>
            <inertial>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <mass value="1.0"/>
                <inertia ixx="1.0" ixy="0" ixz="0" iyy="1.0" iyz="0" izz="1.0"/>
            </inertial>
        </link>

        <joint name="${prefix}joint6" type="revolute">
            <parent link="${prefix}link5"/>
            <child link="${prefix}link6"/>
            <origin xyz="0 0.049 0.0475" rpy="0 0 0"/>
            <axis xyz="0 1 0"/>
            <limit effort="1000" velocity="1.0" lower="${-pi}" upper="${pi}"/>
        </joint>

        <link name="${prefix}tool"/>

        <joint name="${prefix}tool_fixed_joint" type="fixed">
            <parent link="${prefix}link6"/>
            <child link="${prefix}tool"/>
            <origin xyz="0 0.043 0" rpy="${-pi/2} 0 0"/>
        </joint>
    </xacro:macro>
</robot>